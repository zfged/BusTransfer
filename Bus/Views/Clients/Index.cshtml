@model IEnumerable<Bus.Models.Client>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Route.NameRoute)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlaceBus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RoadType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoordsFrom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoordsTo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Route.NameRoute)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FIO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlaceBus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RoadType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoordsFrom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoordsTo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
